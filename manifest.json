{
  "manifest_version": 3,
 
  "name": "Jira Black - Dark Mode",
  "version": "0.3.3",
  "description": "Turn your Jira Cloud to Black, a nice looking Dark Theme.",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.atlassian.net/*", "https://*.herocoders.com/issue/*"],
      "exclude_matches":["https://*.atlassian.net/wiki/*"],
      "js": ["script.js"],
      "css": ["style.css"],
      "all_frames":true
    }
  ]
}